<h2 style="margin:auto"> Move List </h2>
<div class="move-list_col col-sm-6">
  <h3>White</h3>
  <% @white_moves.each_with_index do |move, i| %>
    <li> <%= i+1 %>. <%= move %> </li>
  <% end %>
</div>
<div class="move-list_col col-sm-6">
  <h3>Black</h3>
  <% @black_moves.each_with_index do |move, i| %>
    <li> <%= i+1 %>. <%= move %> </li>
  <% end %>
</div>

